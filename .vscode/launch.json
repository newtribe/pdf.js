{
    "configurations": [
        {
            "name": "Launch pdf verify",
            "program": "${workspaceFolder}/objbrowser/pdfverify.mjs",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "name": "pdf js test runtime",
            "type": "chrome",
            "request": "launch",
            "sourceMaps": false,
            "file": "${workspaceRoot}/web/viewer.html"  
        },
        {
            "name": "pdf render.html",
            "type": "chrome",
            "request": "launch",
            "sourceMaps": false,
            "file": "${workspaceRoot}/objbrowser/render.htm"  
        },
   
        {

                "name": "Launch index.html (disable sourcemaps)",
                "type": "chrome",
                "request": "launch",
                "sourceMaps": false,
                "file": "${workspaceRoot}/objbrowser/index.htm"  
            
        }
    ]

}